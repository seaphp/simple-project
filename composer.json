{
    "name": "seaphp/simple-project",
    "description": "A simple, open source, PHP project.",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Seattle PHP User Group",
            "email": "admin@seaphp.com"
        }
    ],
    "bin": ["bin/calc"],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "SeaPhp\\SimpleProject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SeaPhp\\SimpleProject\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "require": {
        "symfony/console": "~2.5"
    }
}
